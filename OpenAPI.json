{
	"openapi": "2.0",
	"info": {
		"title": "VyatSU schedule api server",
		"contact": {
			"name": "Ali Rzaev",
			"email": "alirzaev1996@gmail.com",
			"url": "vk.me/rzaevali"
		},
		"version": "2.0"
	},
	"servers": [
		{
			"url": "https://vyatsuscheduleapi.herokuapp.com"
		}
	],
	"paths": {
		"/vyatsu/v2/calls": {
			"get": {
				"summary": "Show schedule calls",
				"responses": [
					{
						"200": {
							"description": "a pet to be returned",
							"content": {
								"application/json": {
									"schema": {
										"type": "array",
										"items": {
											"$ref": "#/components/schemas/Call"
										}
									}
								}
							}
						}
					}
				]
			}
		}
	},
	"components": {
		"schemas": {
			"Call": {
				"type": "object",
				"properties": {
					"start": {
						"type": "string"
					},
					"end": {
						"type": "string"
					}
				}
			}
		}
	}
}